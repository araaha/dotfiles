xsct 4000 &
xset r rate 150 50 &
setxkbmap -option caps:ctrl_modifier &
xmodmap -e "keycode 37 = " &
udiskie -N &
greenclip daemon &
env HOME="$XDG_DATA_HOME" mega-cmd-server &
pipewire &
pipewire-pulse &
wireplumber &
pamixer --set-volume 100 &
unclutter --idle 10 &
dunst
